<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="mainViewModel"
            type="com.pnx.momassignment.fragment.MainViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/testUserSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="유저 검색 테스트"
            android:layout_marginTop="30dp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />


        <TextView
            android:id="@+id/saveLocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="로컬 디비 저장"
            android:layout_marginTop="30dp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/testUserSearch"
            app:layout_constraintEnd_toEndOf="parent"
            />


        <TextView
            android:id="@+id/savedids"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="저장된 유져 : "
            android:layout_marginTop="30dp"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/saveLocal"
            app:layout_constraintEnd_toEndOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>